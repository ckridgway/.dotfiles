[user]
	name = Chris Ridgway
	email = ckridgway@gmail.com
[alias]
	co = checkout
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[core]
	excludesfile = ~/.gitignore
[mergetool]
	keepBackup = false
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
